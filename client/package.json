{
  "reactToolbox": {
    "include": [
      "AUTOCOMPLETE",
      "CARD",
      "BUTTON",
      "TOOLTIP",
      "DIALOG",
      "SNACKBAR"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "src/assets/react-toolbox"
  },
  "homepage": "https://www.queerundefined.com",
  "name": "testapp",
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "gh-pages": "^1.0.0",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-masonry-component": "^6.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "0.9.x",
    "react-toolbox": "^2.0.0-beta.12",
    "tapable": "^1.0.0",
    "webpack-core": "^0.6.9"
  },
  "devDependencies": {
    "jest-dom": "^3.1.3",
    "react-test-renderer": "^16.8.3",
    "react-testing-library": "^6.0.3",
    "react-toolbox-themr": "^1.0.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test unit --env=jsdom --setupTestFrameworkScriptFile=./testSetup.js",
    "integration": "react-scripts test integ --env=jsdom --setupTestFrameworkScriptFile=./testSetup.js",
    "eject": "react-scripts eject",
    "toolbox": "react-toolbox-themr"
  },
  "proxy": "http://localhost:5000"
}
